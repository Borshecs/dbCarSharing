<% for(var i = 0; i < transactions.length; i++){ %>
<tr>
    <td><%= i + 1%></td>
    <td><%= transactions[i].Yearofissue %></td>
    <td><%= transactions[i].Price %></td>
    <td><%= transactions[i].Type %></td>
    <td><%= transactions[i].Brand %></td>
    <td>
        <a href="/update_transaction?id=<%=transactions[i]._id%>" class="btn border-shadow update">
            <span class="text-gradient"><i class="fa fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shadow delete" data-id=<%=transactions[i]._id%>>
            <span class="text-gradient"><i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>